  inspection_operation_characteristic_options inspection_operation_characteristic_options[]

  @@unique([attribute_set_id, custom_id], map: "attribute_set_options_attribute_set_id_custom_id_unique")
}

model attribute_sets {
  id                                   BigInt                                 @id @default(autoincrement()) @db.UnsignedBigInt
  plant_id                             BigInt                                 @db.UnsignedBigInt
  custom_id                            String                                 @db.VarChar(255)
  created_at                           DateTime?                              @db.Timestamp(0)
  updated_at                           DateTime?                              @db.Timestamp(0)
  internal_id                          String?                                @db.VarChar(255)
  is_active                            Boolean                                @default(true)
  name                                 String?                                @db.VarChar(255)
  attribute_set_options                attribute_set_options[]
  plants                               plants                                 @relation(fields: [plant_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "attribute_sets_plant_id_foreign")
  inspection_operation_characteristics inspection_operation_characteristics[]

  @@unique([plant_id, custom_id], map: "attribute_sets_plant_id_custom_id_unique")
}

model backlog_item_weeks {
  id               BigInt        @id @default(autoincrement()) @db.UnsignedBigInt
  backlog_item_id  BigInt        @db.UnsignedBigInt
  week             Int
  year             Int
  qty_prod_order   Float
  qty_call_off     Float
  qty_stock        Float
  qty_backlog      Float
  qty_call_off_sim Float
  qty_stock_sim    Float
  qty_backlog_sim  Float
