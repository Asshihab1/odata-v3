generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model absence_requests {
  id                                        BigInt         @id @default(autoincrement()) @db.UnsignedBigInt
  user_id                                   BigInt         @db.UnsignedBigInt
  absence_type_id                           BigInt?        @db.UnsignedBigInt
  start                                     DateTime       @db.DateTime(0)
  end                                       DateTime       @db.DateTime(0)
  total_hour                                Float?
  status                                    String         @db.VarChar(255)
  approved_by                               BigInt?        @db.UnsignedBigInt
  applicant_note                            String?        @db.Text
  supervisor_note                           String?        @db.Text
  approval_time                             DateTime?      @db.DateTime(0)
  created_at                                DateTime?      @db.Timestamp(0)
  updated_at                                DateTime?      @db.Timestamp(0)
  is_read                                   Boolean        @default(false)
  graph_id                                  String?        @db.VarChar(255)
  is_full_day                               Boolean        @default(false)
  absence_types                             absence_types? @relation(fields: [absence_type_id], references: [id], onUpdate: NoAction, map: "absence_requests_absence_type_id_foreign")
  users_absence_requests_approved_byTousers users?         @relation("absence_requests_approved_byTousers", fields: [approved_by], references: [id], onUpdate: NoAction, map: "absence_requests_approved_by_foreign")
  users_absence_requests_user_idTousers     users          @relation("absence_requests_user_idTousers", fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "absence_requests_user_id_foreign")

  @@index([absence_type_id], map: "absence_requests_absence_type_id_foreign")
  @@index([approved_by], map: "absence_requests_approved_by_foreign")
  @@index([user_id], map: "absence_requests_user_id_foreign")
}

model absence_types {
  id               BigInt             @id @default(autoincrement()) @db.UnsignedBigInt
  custom_id        String             @unique(map: "absence_types_custom_id_unique") @db.VarChar(255)
  is_active        Boolean            @default(false)
  created_at       DateTime?          @db.Timestamp(0)
  updated_at       DateTime?          @db.Timestamp(0)
  absence_requests absence_requests[]
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model action_meetings {
  action_setting_id BigInt          @db.UnsignedBigInt
  meeting_id        BigInt          @db.UnsignedBigInt
  action_settings   action_settings @relation(fields: [action_setting_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "action_meetings_action_setting_id_foreign")
  meetings          meetings        @relation(fields: [meeting_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "action_meetings_meeting_id_foreign")

  @@index([action_setting_id], map: "action_meetings_action_setting_id_foreign")
  @@index([meeting_id], map: "action_meetings_meeting_id_foreign")
  @@ignore
}

model action_settings {
  id              BigInt            @id @default(autoincrement()) @db.UnsignedBigInt
  name            String            @db.VarChar(255)
  sort_order      Int?              @default(0)
  color           String            @db.VarChar(255)
  created_at      DateTime?         @db.Timestamp(0)
  updated_at      DateTime?         @db.Timestamp(0)
  action_meetings action_meetings[] @ignore
  tasks           tasks[]
}

model area_user {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  area_id    BigInt?   @db.UnsignedBigInt
  user_id    BigInt?   @db.UnsignedBigInt
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
  areas      areas?    @relation(fields: [area_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "area_user_area_id_foreign")
  users      users?    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "area_user_user_id_foreign")

  @@index([area_id], map: "area_user_area_id_foreign")
  @@index([user_id], map: "area_user_user_id_foreign")
}

model areas {
  id         BigInt      @id @default(autoincrement()) @db.UnsignedBigInt
  is_active  Boolean     @default(true)
  custom_id  String      @unique(map: "areas_custom_id_unique") @db.VarChar(100)
  name       String      @db.VarChar(255)
  created_at DateTime?   @db.Timestamp(0)
  updated_at DateTime?   @db.Timestamp(0)
  area_user  area_user[]
}

model attribute_set_options {
  id                                          BigInt                                        @id @default(autoincrement()) @db.UnsignedBigInt
  attribute_set_id                            BigInt                                        @db.UnsignedBigInt
  custom_id                                   String                                        @db.VarChar(255)
  valuation                                   String                                        @db.VarChar(255)
